            <!-- page content -->
            <div class="right_col" role="main">


              <div class="row">

                <div class="col-md-6 m-b-10">
                  <div class="input-group">
                    <input id="ref_url" class="form-control" name="ref_url" type="text" value="/Signup?ref=<%= username %>">
