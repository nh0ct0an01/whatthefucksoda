                  <span class="count_top" id="balance-abc-in-usd"><i class="fa fa-user"></i> ACB </span>
                  <div class="count" id="U_ACB"><%= balanceACB %></div>
                </div>
                <div class="col-md-2 col-sm-4 col-xs-6 tile_stats_count">
                  <span class="count_top"><i class="fa fa-clock-o"></i> BTC </span>
                  <div class="count"  id="U_BTC"><%= balanceBTC %></div>
                </div>
              </div>


